    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="net.orpiske.maestro.results.main.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>